{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port=8000 --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "//": "New scripts to run and build Storybook with Tailwind",
    "storybook": "concurrently \"yarn:watch:*\"",
    "build-storybook": "concurrently \"yarn:build:*\"",
    "build:css": "npx tailwindcss -i ./src/index.css -o ./public/index.css",
    "build:storybook": "storybook build",
    "watch:css": "npx tailwindcss -i ./src/index.css -o ./public/index.css --watch",
    "watch:storybook": "start-storybook dev -p 6006"
  },
  "dependencies": {
    "axios": "^1.2.1",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.5.0",
    "socket.io-client": "^4.5.4",
    "tailwindcss": "^3.2.4",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
